{
  "Transfers": [
    {
      "Name": "Adelphic No Bid Logs S3 -> GS",
      "Source": {
        "Name": "s3://r.ci.com/adlog.request/<dateFormat:yyyy>/<dateFormat:MM>/<dateFormat:dd>/<dateFormat:HH>/",
        "Type": "url",
        "CredentialFile": "/etc/etly/secret/s3.json",
        "DataFormat": "ndjson",
        "Compression": "gzip",
        "FilterRegExp": ".gz$",
        "DataType": "AdelphicNoBidLog"
      },
      "Target": {
        "Name": "gs://s3adlogs/ad_rtb_nobids/$date/<mod:40>/<dateFormat:HH>-<file>",
        "Type": "url",
        "CredentialFile": "/etc/etly/secret/gs.json",
        "DataFormat": "ndjson",
        "Compression": "gzip"
      },
      "Meta": {
        "Name": "gs://s3adlogs/ad_rtb_nobids/meta/<dateFormat:yyyy><dateFormat:MM><dateFormat:dd>-nobid-transfer-meta.json",
        "CredentialFile": "/etc/etly/secret/gs.json"
      },
      "TimeWindow": {
        "Duration": 2,
        "Unit": "hour"
      },
      "TimeOut": {
        "Duration": 6,
        "Unit": "min"
      },
      "Frequency": {
        "Duration": 2,
        "Unit": "sec"
      },
      "VariableExtraction": [
        {
          "Name": "$date",
          "Source": "target",
          "Provider": "requestDate"
        }
      ],
      "Filter": "NoBidAdelphicLogFilter",
      "Transformer": "AdelphicNoBidLogTransformer",
      "MaxParallelTransfers": 256,
      "MaxTransfers": 1500,
      "FailRetry": 3
    }
  ]
}
